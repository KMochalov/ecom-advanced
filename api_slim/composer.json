{
    "name": "alcoweb/ecom-advanced",
    "type": "project",
    "description": "ecom engene",
    "authors": [
        {
            "name": "Kostya Mochalov"
        }
    ],
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "php-di/php-di": "^7.0",
        "ramsey/uuid": "^4.7",
        "slim/psr7": "^1.6",
        "slim/slim": "^4.12",
        "symfony/console": "^7.0",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "overtrue/phplint": "^9.1",
        "phpunit/phpunit": "^10.5",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.8",
        "vimeo/psalm": "^5.18"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "scripts": {
        "lint": "phplint",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "psalm": "psalm",
        "console": "php console/app.php --ansi"
    }
}
